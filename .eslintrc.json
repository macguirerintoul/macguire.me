{
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"next/core-web-vitals",
		"plugin:prettier/recommended"
	],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn"
  },
	"plugins": ["@typescript-eslint"]
}
